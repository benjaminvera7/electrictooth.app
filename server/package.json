  "dependencies": {
    "adm-zip": "^0.5.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "formidable": "^1.2.2",
    "jszip": "^3.5.0",
    "jwt-simple": "^0.5.5",
    "mongoose": "^5.4.10",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.4",
    "nodemon": "^1.18.9",
    "paypal-rest-sdk": "^1.8.1"
  },
